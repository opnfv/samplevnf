[DEFAULT]
name = impairTesting
number_of_tests = 1
total_number_of_vms = 3
init_code=init_test()
dest_vm = not_used
gw_vm = not_used
script_control = false
group1cores = not_used
group2cores = not_used
group3cores = not_used

[VM1]
name = Generator
config_file = gen_gw.cfg
gw_vm = 2
dest_vm = 3
