[DEFAULT]
name = GWTesting
number_of_tests = 2
total_number_of_vms = 3
init_code=init_test()
dest_vm = not_used
gw_vm = not_used
script_control = false
group1cores = not_used
group2cores = not_used
group3cores = not_used
drop_rate_treshold = 0.01
accuracy = 0.01

[VM1]
name = Generator
config_file = gen_gw.cfg
dest_vm = 3
gw_vm = 2
script_control = true
group1cores = [1]
group2cores = [3]
group3cores = [1,3]

[VM2]
name = GW1
config_file = secgw1.cfg
dest_vm = 3
group1cores = [1]

[VM3]
name = GW2
config_file = secgw2.cfg
group1cores = [1]

[test1]
cmd=run_speedtest(sock[0],sock[2])

[test2]
cmd=run_sizetest(sock[0],sock[2])
